{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "release-type": "node",
      "include-v-in-tag": false,
      "changelog-path": "CHANGELOG.md",
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features"
        },
        {
          "type": "fix",
          "section": "Bug Fixes"
        },
        {
          "type": "perf",
          "section": "Performance"
        },
        {
          "type": "deps",
          "section": "Dependencies"
        },
        {
          "type": "docs",
          "section": "Documentation"
        },
        {
          "type": "style",
          "section": "Styles"
        },
        {
          "type": "refactor",
          "section": "Refactoring"
        },
        {
          "type": "test",
          "section": "Tests"
        },
        {
          "type": "ci",
          "section": "CI/CD"
        },
        {
          "type": "Build",
          "section": "Build"
        },
        {
          "type": "chore",
          "section": "Miscellaneous",
          "hidden": true
        }
      ]
    }
  }
}
